.backgroundBox{
  width: 100%;
  height: 100%;
  background-color: #CCCCCC;
  position: relative;
}
.backgroundBox .backgroundImg{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;left: 0;
  z-index:99;
  overflow: hidden;
  background-color:#848484;
}
.backgroundBox .content{
  width: 100%;
  height: 100%;
  position: absolute;
  background-color: rgba(9, 9, 9, 0.6);
  top: 0;left: 0;
  z-index:999;
  overflow: hidden;

}


