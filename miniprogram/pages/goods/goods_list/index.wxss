}
.filtrate view:nth-child(2) text{
  display: inline-block;
  transform: translate(4px,1px);
  font-size: 16px;
}
.filtrate view.active{
  color:#047ED6;
  font-weight: 600;
}
.filtrate view.active text.rotatesUp{
  transform:rotate(180deg);
}
