/* 横列 */
.goods_strip_item{
  width: 100%;
  background-color: #ffffff;
}
.goods_strip_item .imgs{
  width: 33%;
  height:228rpx;
  overflow: hidden;
  border-radius:20rpx;
  margin-bottom: 40rpx;
  position: relative;
  float: left;
  margin-right:18px;
}
.goods_strip_item .imgs image:nth-child(1){
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  z-index: 99;
}

.goods_strip_item .imgs image:nth-child(2){
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  z-index: 1;
}
.goods_strip_item .textOver{
  width: 100%;
  display: inline-block;
  overflow:hidden;
	white-space:nowrap;
	text-overflow: ellipsis;
}
.goods_strip_item .stripRight_box{
  width: 60%;
  float: left;
  height: 228rpx;
  position: relative;
}
.goods_strip_item .stripRight_box .item_title{
  font-size:13px;
  overflow:hidden; 
  text-overflow:ellipsis;
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2; 
}
.goods_strip_item .stripRight_box .iconfont{
  position: absolute;
  bottom: 0;
  right: 0;
  font-size: 50rpx;
}
.toStripImg{
  width: 100%;
  padding-bottom:95px;
}
.toStripImg view{
  float: left;
  width: 220rpx;
  height: 220rpx;
  overflow: hidden;
  margin: 4rpx;
  border: 4rpx solid #ffffff;
  position: relative;
}
.toStripImg view .iconfont{
  position: absolute;
  right: 5rpx;
  bottom: 5rpx;
  color: #007AFF;
  z-index: 999;
}
.toStripImg view.active{
  border-color:#307EF8;
}
.toStripImg view image{
  position:absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
}
.toStripImg view image:nth-child(1){
  z-index: 99;
}
.toStripImg view image:nth-child(2){
  z-index: 9;
}

